Notifier {
  padding: 10px
  display: block
  text-align: center
  animation: 0.5s slide-in
  background: rgb(214, 228, 236)
  border-bottom: 1px solid rgb(187, 201, 210)
  position: relative
  :hover {
    background: rgb(220, 242, 255)
  }
  -loader {
    background: rgb(255, 239, 217)
    border-bottom: 1px solid rgb(228, 220, 195)
    color: #10100c !important
    :hover {
      background: rgb(245, 229, 207)
    }
  }s
}
