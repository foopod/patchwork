ConfirmSuggest {
  position: fixed
  border: 1px solid #ddd
  z-index: 100
  background: var(--secondary-bg)
  ul {
    margin: 0
    padding: 0
    list-style: none
    li {
      padding: 4px 8px
      font-size: 85%
      border-bottom: 1px solid #ddd
      selected {
        color: #fff
        background-color: #428bca
        border-color: darken(#428bca, 5%)
      }
      img {
        height: 20px
      }
      :last-child {
        border: 0
      }
    }
  }
}
